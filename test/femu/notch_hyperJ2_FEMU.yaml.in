notch hyperJ2 FEMU:

  problem:
    name: notch_hyperJ2_FEMU
    num steps: 4
    step size: 1.

  discretization:
    geom file: '@CMAKE_SOURCE_DIR@/test/mesh/notch/notch.dmg'
    mesh file: '@CMAKE_BINARY_DIR@/test/femu/notch_hyperJ2_synthetic/'
    assoc file: '@CMAKE_SOURCE_DIR@/test/mesh/notch/notch.txt'

  residuals:
    global residual:
      type: 'mechanics'
      nonlinear max iters: 15
      nonlinear absolute tol: 1.e-8
      nonlinear relative tol: 1.e-8
      print convergence: false
    local residual:
      type: 'J2'
      nonlinear max iters: 500
      nonlinear absolute tol: 1.e-12
      nonlinear relative tol: 1.e-12
      materials:
        body:
          E: 1000.
          nu: 0.25
          K: 100.
          Y: 10.

  quantity of interest:
    type: 'surface mismatch'
    side set: 'zmax'

  inverse:
    gradient type: "FEMU"
    iteration limit: 0
    materials:
      body:
        E: [800., 1000.]
        nu: [0.2, 0.3]
        K: [90., 110.]
        Y: [9., 11.]

  # bc name: [resid_idx, eq, node_set_name, value]
  dirichlet bcs:
    bc 1: [0, 0, xmin, 0.0]
    bc 2: [0, 1, ymin, 0.0]
    bc 3: [0, 2, zmin, 0.0]
    bc 4: [0, 1, ymax, 0.001 * t]

  linear algebra:
    krylov size: 1000
    max iters: 1000
    tolerance: 1.0e-8
    output frequency: 0
    multigrid:
      verbosity: none
      number of equations: 4
      "problem: symmetric": false
