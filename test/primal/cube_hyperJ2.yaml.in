cube hyperJ2:

  problem:
    name: cube_hyperJ2
    num steps: 10
    step size: 1.

  discretization:
    geom file: '@CMAKE_SOURCE_DIR@/test/mesh/cube/cube.dmg'
    mesh file: '@CMAKE_SOURCE_DIR@/test/mesh/cube/cube.smb'
    assoc file: '@CMAKE_SOURCE_DIR@/test/mesh/cube/cube.txt'

  residuals:
    global residual:
      type: 'mechanics'
      nonlinear max iters: 15
      nonlinear absolute tol: 1.e-8
      nonlinear relative tol: 1.e-8
      print convergence: true
    local residual:
      type: 'J2'
      nonlinear max iters: 30
      nonlinear absolute tol: 1.e-12
      nonlinear relative tol: 1.e-12
      materials:
        body:
          E: 1000.
          nu: 0.25
          K: 100.
          Y: 10.

  # bc name: [resid_idx, eq, node_set_name, value]
  dirichlet bcs:
    bc 1: [0, 0, xmin, 0.0]
    bc 2: [0, 1, ymin, 0.0]
    bc 3: [0, 2, zmin, 0.0]
    bc 4: [0, 1, ymax, 0.01 * t]

  quantity of interest:
    type: 'average displacement'

  regression:
    QoI: 1.57817536611772440e-02
    relative error tol: 1.0e-4

  linear algebra:
    krylov size: 1000
    max iters: 1000
    tolerance: 1.0e-8
    output frequency: 0
    multigrid:
      verbosity: none
      number of equations: 4
      "problem: symmetric": false
